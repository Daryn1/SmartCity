@model List<SmartCity.Web.Models.Student.BucsTeamCaptainViewModel>

<div>
    <h1>TAMPA BAY BUCCANEERS TEAM CAPTAINS</h1>
</div>

@foreach (var captain in Model)
{
    <div>
        <br>
        <img src="@captain.PhotoURL" alt="Not Available"/>
    </div>
    <div>
        <b>@captain.Name</b>
    </div>
    <div>
        <b>Position: </b>@captain.Position
    </div>
    <div>
        <b>Experience: </b>@captain.Experience
    </div>
    <div>
        <b>College: </b>@captain.College
    </div>
 }
