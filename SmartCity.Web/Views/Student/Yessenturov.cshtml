@model List<SmartCity.Web.Models.Student.ProductsViewModel>
<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script src="https://netdna.bootstrapcdn.com/twitter-bootstrap/2.0.4/js/bootstrap-alert.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.success').click(function (e) {
            e.preventDefault()
            $('.message').html('<div class="alert alert-success fade in"><button type="button" class="close close-alert" data-dismiss="alert" aria-hidden="true">×</button>Success! Product added to cart.</div>');
        })

    });
</script>

<div id="img1"><img src="data:image/png;base64," /></div>
<h1>Welcome to Kazakh Technologies.</h1>
<h2>We provide several new generation grade products and we are looking for tight partnership in terms of hardware.</h2>
<h3>Catalog:</h3>
<div class="message"></div>
@foreach (var prod in Model)
{
    <div class="card">
        @if (prod.imgUrl != null)
        {
            <div>
                <img src="@prod.imgUrl" alt="@prod.Description" style="width:100%">
            </div>
        }

        <h1>@prod.Name</h1>
        <p class="price">$@prod.Price</p>
        <p>@prod.Description</p>
        <p><button class="success">Add to Cart</button></p>

    </div>


}
